{
    "name": "thecodingmachine/lazy-array",
    "type": "library",
    "description": "This package provides an array than can lazily instantiate the objects it contains.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\LazyArray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\LazyArray\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
